<?xml version="1.0" encoding="UTF-8"?>
<phpunit colors="true">
    <filter>
        <blacklist>
            <exclude>
                <directory suffix=".php">./vendor/</directory>
                <file>*</file>
            </exclude>
        </blacklist>
    </filter>
    <testsuites>
        <testsuite name="Application Test Suite">
            <directory>./Test/</directory>
        </testsuite>
    </testsuites>
</phpunit>